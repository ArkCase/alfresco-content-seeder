<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="patch.armediaContentSeeder" class="com.armedia.alfresco.extension.contentseeder.ContentSeeder" parent="basePatch">
		<property name="id" value="com.armedia.alfresco.extension.patch.contentSeeder"/>
		<property name="description" value="com.armedia.alfresco.extension.patch.contentSeeder.description"/>
		<property name="fixesFromSchema" value="0"/>
		<property name="fixesToSchema" value="1"/>
		<property name="targetSchema" value="2"/>
    </bean>

</beans>